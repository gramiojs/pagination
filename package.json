{
	"name": "@gramio/pagination",
	"version": "0.0.8",
	"description": "Pagination library for telegram bots",
	"keywords": [
		"gramio",
		"telegram",
		"pagination",
		"tma",
		"bot",
		"telegram-pagination",
		"paginator",
		"menu",
		"telegram-menu",
		"bot-menu"
	],
	"homepage": "https://github.com/gramiojs/pagination",
	"readme": "https://github.com/gramiojs/pagination",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/gramiojs/pagination.git"
	},
	"author": {
		"url": "https://github.com/kravetsone",
		"name": "kravets"
	},
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			},
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			}
		},
		"./plugin": {
			"import": {
				"types": "./dist/plugin.d.ts",
				"default": "./dist/plugin.js"
			},
			"require": {
				"types": "./dist/plugin.d.cts",
				"default": "./dist/plugin.cjs"
			}
		}
	},
	"scripts": {
		"prepublishOnly": "bunx pkgroll"
	},
	"type": "module",
	"files": ["dist"],
	"license": "MIT",
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/bun": "^1.2.8",
		"gramio": "^0.4.7",
		"pkgroll": "^2.11.2",
		"typescript": "^5.8.2"
	},
	"dependencies": {
		"@gramio/callback-data": "^0.0.11",
		"@gramio/keyboards": "^1.2.1",
		"@gramio/types": "^9.0.1"
	},
	"peerDependencies": {
		"gramio": "^0.4.7"
	},
	"peerDependenciesMeta": {
		"gramio": {
			"optional": true
		}
	}
}
